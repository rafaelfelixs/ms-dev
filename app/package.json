{
  "name": "ms-dev",
  "version": "1.0.0",
  "description": "MS Dev with db and redis connections",
  "main": "index.js",
  "scripts": {
    "lint": "eslint  --ext .ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --check \"src/**/*.+(js|ts|json)\"",
    "format:fix": "prettier --write \"src/**/*.+(js|ts|json)\"",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "debug": "ts-node-dev --inspect=0.0.0.0 --respawn --transpile-only src/index.ts",
    "build-api": "tsc",
    "migrate:generate": "typeorm-ts-node-commonjs -d src/Domain/Configure/AppDataSource.ts migration:generate src/Domain/Migrations/$npm_config_name",
    "migrate:create": "typeorm-ts-node-commonjs migration:create src/Domain/Migrations/$npm_config_name",
    "migrate:run": "typeorm-ts-node-commonjs -d src/Domain/Configure/AppDataSource.ts migration:run",
    "migrate:revert": "typeorm-ts-node-commonjs -d src/Domain/Configure/AppDataSource.ts migration:revert",
    "schema:drop": "typeorm-ts-node-commonjs -d src/Domain/Configure/AppDataSource.ts schema:drop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mysql2": "^3.4.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "tsc": "^2.0.4"
  }
}
